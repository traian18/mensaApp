<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mensaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AboutIqTest"
            android:label="@string/title_activity_about_iq_test"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".AboutMensaRomania"
            android:label="@string/title_activity_about_mensa_romania"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".WhatIsIqActivity"
            android:label="@string/title_activity_what_is_iq"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AboutMensaInternational"
            android:label="@string/title_activity_about_mensa_international"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TestActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>